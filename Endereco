public class Endereco {
    
    public String codigo;
    public String logradouro;
    public float rua;
    public String cidade;
    public String estado;
        
     Endereco(String codigo, String logradouro, float rua, String cidade, String estado) {
        
        this.codigo = codigo;
        this.logradouro = logradouro;
        this.rua = rua;
        this.cidade = cidade;
        this.estado = estado;
    }
    
    public void status () {
        
        System.out.println("Dados Do Caixa Eletronico :");
        System.out.println("Codigo:     " + this.codigo);
        System.out.println("Logradouro: " + this.logradouro);
        System.out.println("Num da Rua: " + this.rua);
        System.out.println("Cidade:   " +this.cidade + " - Estado de " +this.estado);
        
    }
    
    
}
